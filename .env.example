## IMPORTANT: GIT_REMOTE_URL must point to a PRIVATE REPO
# Git remote for syncing flows
GIT_REMOTE_URL='https://<token>@github.com/<your-username>/<your-repo>.git'

# How often to sync flows (minutes)
SYNC_INTERVAL_MINUTES=1

# List of Flowise instances to sync
# Instance JSON structure for FLOWISE_INSTANCES_JSON:
# [
#   {
#     "name": "production",
#     "url": "https://flowise-prod.instance.com",
#     "apiKey": "your-production-api-key",
#     "enabled": false
#   },
#   {
#     "name": "staging",
#     "url": "https://flowise-staging.instance.com",
#     "apiKey": "your-staging-api-key",
#     "enabled": false
#   },
#   {
#     "name": "development",
#     "url": "https://flowise-dev.instance.com",
#     "apiKey": "your-development-api-key",
#     "enabled": true
#   }
# ]
FLOWISE_INSTANCES_JSON='[{"name":"development","url":"https://your-development-url","apiKey":"your-development-api-key","enabled":true},{"name":"staging","url":"https://your-staging-url","apiKey":"your-staging-api-key","enabled":false}]'